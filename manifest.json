{
    "manifest_version": 3,
    "name": "YouTube Ad Skip",
    "description": "Automatically skips video ads and closes banner ads on YouTube.",
    "version": "1.2.0",
    "icons": { 
        "16": "ad-skip-icon-16.png",
        "32": "ad-skip-icon-32.png",
        "48": "ad-skip-icon-48.png",
       "128": "ad-skip-icon-128.png" },
    "content_scripts" : [
        {
          "matches": ["*://*.youtube.com/*"],
          "js": ["skip.js"]
        }
      ]
  }